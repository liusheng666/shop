<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cssl.dao.SpThreeDao">
    <select id="threeList" resultType="Spthree">
 		select * from spthree where sptwocode=#{twocode}
 	</select>
	<select id="three2List" resultType="Spthree">
 		select * from spthree where sponecode=#{onecode} limit 0,12
 	</select>
</mapper>
