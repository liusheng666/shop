<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cssl.dao.SpTwoDao">
    <select id="twoList" resultType="Sptwo">
 		select * from sptwo where sponecode=#{onecode}
 	</select>
	<select id="two2List" resultType="Sptwo">
 		select * from sptwo where sponecode=#{onecode} limit 0,6
 	</select>
</mapper>
